{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "ie >=11",
            "last 2 version",
            "> 5%",
            "not dead"
          ]
        },
        "corejs": "3",
        "useBuiltIns": "usage"
      }
    ],
    [
      "react-app",
      {
        "absoluteRuntime": false
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-nullish-coalescing-operator"
    ],
    [
      "@babel/plugin-proposal-optional-chaining"
    ]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}